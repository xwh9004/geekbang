<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="user1" class="com.eample.spring.ioc.overview.dependency.domin.User">
        <property name="id" value="1"></property>
        <property name="name" value="Jesse"></property>
    </bean>
    <bean id="superUser" class="com.eample.spring.ioc.overview.dependency.domin.SuperUser" parent="user1" primary="true">
        <property name="address" value="jiangxi"></property>
    </bean>


    <bean id="objectFactory" class="org.springframework.beans.factory.config.ObjectFactoryCreatingFactoryBean">
        <property name="targetBeanName" value="user1"></property>
    </bean>
</beans>